<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.controller.DashboardController">
   <children>
      <Button layoutX="86.0" layoutY="112.0" mnemonicParsing="false" onAction="#showProfile" text="Profile">
         <font>
            <Font name="Lato Black" size="18.0" />
         </font>
      </Button>
      <Text layoutX="219.0" layoutY="68.0" strokeType="OUTSIDE" strokeWidth="0.0" text="DASHBOARD">
         <font>
            <Font name="Lato-Light" size="24.0" />
         </font>
      </Text>
      <Button fx:id="singleplayerBtn" layoutX="86.0" layoutY="155.0" mnemonicParsing="false" onAction="#startSinglegame" text="Single Player">
         <font>
            <Font name="Lato Black" size="18.0" />
         </font>
      </Button>
      <Button fx:id="multiplayerBtn" layoutX="235.0" layoutY="155.0" mnemonicParsing="false" onAction="#startMultiplayer" text="Multi Player">
         <font>
            <Font name="Lato Black" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
